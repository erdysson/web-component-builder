{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text-summary",
    "html"
  ],
  "report-dir": "coverage",
  "include": [
    "src/**/*.ts"
  ],
  "extension": [
    ".ts"
  ],
  "exclude": [
    "node_modules/**",
    "test/**/*.ts"
  ]
}
